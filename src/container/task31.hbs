<main>
    <heder>
        {{#with navigation}}
        <nav>
            {{#each links}}
            <a class="btn btn-link" href="{{href}}">{{text}}</a>
            {{/each}}

            <a class="btn btn-primary" href="{{button.href}}">{{button.text}}</a>
            <hr>
        </nav>
        {{/with}}

        {{#with header}}
        <h1>{{title}}</h1>

        <p>{{description}}</p>

        <a class="btn btn-primary" href="button.link">{{button.text}}</a>
        {{/with}}
        <hr>
    </heder>

    {{#with city}}
    <section class="row gy-4 gx-2">
        <h2>{{title}}</h2>

        {{#each cards}}
        <div class="col-5">
            <div class="card">
                {{#with picture}}
                <img class="card-image-top" src={{src}} alt={{alt}}>
                {{/with}}
                <div class="card-body">
                    <h6 class="card-title">{{title}}</h6>
                    <p>{{description}}</p>
                    {{#with button}}
                    <a class="btn btn-primary btn-lg" href={{href}}>{{text}}</a>
                    {{/with}}
                </div>
            </div>
        </div>

        {{/each}}
        <hr>
    </section>
    {{/with}}

    {{#with winner}}
    <section>
        <h2>{{title}}</h2>
        {{#with table}}
        <table class="table">
            <thead>
                <tr>
                    {{#each head}}
                    <th>{{this}}</th>
                    {{/each}}
                </tr>
            </thead>
            {{#each user}}
            <tr>
                <td>{{place}}</td>
                <td>{{name}}</td>
                <td>{{surname}}</td>
                <td>{{nickname}}</td>
            </tr>
            {{/each}}
        </table>
        {{/with}}
        <hr>
    </section>
    {{/with}}

    {{#with gallery}}
    <section>
        <h2>{{title}}</h2>
        <br>
        <div class="row g-2">
            {{#each cards}}

            <div class="col-4">
                <div class="card">
                    <div class="card-header">
                        <div class="card-title">{{name}}</div>
                    </div>
                    <ul class="list-group">
                        {{#each items}}
                        <li class="list-group-item">{{this}}</li>
                        {{/each}}
                    </ul>
                </div>
            </div>

            {{/each}}
        </div>
        <hr>
    </section>
    {{/with}}

    {{#with relaxion}}
    <section>
        <div class="display-3">{{title}}</div>
        <br>
        <div class="row gx-2 gy-4">
            {{#each cards}}
            <div class="col-5">
                <div class="card text-bg-secondary">
                    <div class="card-header">{{header}}</div>
                    <div class="card-body">
                        <h6 class="card-title">{{title}}</h6>
                        <p>{{description}}</p>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
        <hr>
    </section>
    {{/with}}

    {{#with rest}}
    <section>
        <br>
        <div class="row g-2">
            {{#each cards}}
            <div class="col-6">
                <div class="card text-center">
                    <div class="card-header">{{header}}</div>
                    <div class="card-body">
                        <h6 class="card-title">{{title}}</h6>
                        <p>{{description}}</p>
                        <button class="btn btn-primary btn-lg">{{button}}</button>
                    </div>
                    <div class="card-footer">{{footer}}</div>
                </div>
            </div>
            {{/each}}
        </div>
        <hr>
    </section>
    {{/with}}

    {{#with proposition}}
    <section>
        <div class="display-2">{{title}}</div>
        <br>
        <div class="row g-2">
            {{#each card}}
            <div class="col-5">
                <div class="card text-bg-warning text-light">
                    <div class="card-header">{{header}}</div>
                    <div class="card-body">
                        <h6 class="card-title">{{title}}</h6>
                        <p>{{description}}</p>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
        <hr>
    </section>
    {{/with}}

    {{#with footer}}
    <footer>
        <h5>{{title}}</h5>
        <br>
        <div class="row gx-5 gy-2">
            <div class="col-1">
                {{#each navigation}}
                <a class="btn btn-link" href={{href}}>{{text}}</a>
                {{/each}}
            </div>
            <div class="col-1">
                {{#each socialMedia}}
                <a class="btn btn-outline-dark" href={{link}}>{{name}}</a>
                {{/each}}
            </div>
            <div class="col-1">
                {{#each partners}}
                <a class="btn btn-info" href={{link}}>{{name}}</a>
                {{/each}}
            </div>
        </div>
    </footer>
    {{/with}}
</main>