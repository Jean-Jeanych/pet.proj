<main>
    {{#with page_header}}
    <header>
        <h1 class="text-center">{{title}}</h1>
        <p class="text-uppercase text-center">{{description}}</p>
    </header>
    {{/with}}

    {{#with section}}
    <section>
        {{#with about_us}}
        <div class="card">
            <div class="card-body">
                {{#with title_list}}
                <div class="display-6 text-end text-dark">{{main}}</div>
                <br>
                <h2 class="text-end">{{sub}}</h2>
                <br>
                {{/with}}
                <button class="btn btn-outline-info btn-sm">{{button.text}}</button>
            </div>
        </div>
        {{/with}}
        <br>
        {{#with advantage}}
        <div class="card">
            <div class="card-body">
                <div class="display-5 text-secondary">{{title}}</div>
                <br>
                {{#with efficiency}}
                <div class="card">
                    <div class="card-body">
                        <div class="display-4 text-secondary">{{title}}</div>
                        <br>
                        <h6 class="text-secondary">{{sub_title}}</h6>
                        <br>
                        <p>{{description}}</p>
                        <button class="btn btn-warning btn-sm">{{button}}</button>
                    </div>
                </div>
                {{/with}}
                <br>
                {{#with innovation}}
                <div class="card">
                    <div class="card-body">
                        <div class="display-4 text-center text-secondary">{{title}}</div>
                        <br>
                        <h6 class="text-center">{{sub_title}}</h6>
                        <br>
                        <p class="text-center">{{description}}</p>
                        <button class="btn btn-warning btn-sm">{{button}}</button>
                    </div>
                </div>
                {{/with}}
                <br>
                {{#with collaboration}}
                <div class="card">
                    <div class="card-body">
                        <div class="display-4 text-end text-secondary">{{title}}</div>
                        <br>
                        <h6 class="text-end">{{sub_title}}</h6>
                        <br>
                        <p class="text-end">{{description}}</p>
                        <button class="btn btn-warning btn-sm">{{button}}</button>
                    </div>
                </div>
                {{/with}}
            </div>
        </div>
        {{/with}}
        <br>
        {{#with agreement}}
        <div class="card">
            <div class="card-body">
                <div class="display-5">{{title}}</div>
                <br>
                {{#with info}}
                <div class="card">
                    <div class="card-body">
                        <h5>{{sub_title}}</h5>
                        <br>
                        {{#with detail}}
                        <div class="card">
                            <div class="card-body">
                                <p>{{text}}</p>
                                <button class="btn btn-success">{{button}}</button>
                            </div>
                        </div>
                        {{/with}}
                    </div>
                </div>
                {{/with}}
            </div>
        </div>
        {{/with}}
    </section>
    {{/with}}
</main>