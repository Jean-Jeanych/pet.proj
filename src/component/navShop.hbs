<div class="card mt-3 mb-5 ms-2">
    {{#with navigation}}
    <div class="nav">
        {{#each links}}
        <div class="nav-item">
            <a href={{href}} class="nav-link">{{text}}</a>
        </div>
        {{#if @last}}
        <div class="nav-item">
            <a href={{href}} class="nav-link disabled">{{text}}</a>
        </div>
        {{/if}}
        {{/each}}
    </div>
    {{/with}}
</div>